<Window x:Class="LyPlan.MasterControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LyPlan"
        mc:Ignorable="d"
        Title="LyPlan Control" Height="100" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Activated="Window_Activated">
    <Grid Background="#FF333333" >
        <TextBlock Text="LyPlan is running..." Style="{StaticResource menu-style}" Margin="20,0,0,0"></TextBlock>
    </Grid>
</Window>
